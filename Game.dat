<?xml version="1.0" encoding="utf-8"?>
<obprofiles xmlns="http://www.logitech.com/Cassandra/2013.1/OnboardProfile">
  <profile name="Game">
    <device anglesnapping="0" model="Logitech.Gaming.Mouse.G700s" buttoncount="13" powermode="3" reportrate="500">
      <dpitable currentindex="1" defaultindex="1">
        <dpi x="400" y="400"/>
        <dpi x="800" y="800"/>
        <dpi x="1200" y="1200"/>
        <dpi x="1600" y="1600"/>
        <dpi x="3200" y="3200"/>
      </dpitable>
    </device>
    <buttons>
      <button name="Left Click" number="1">
        <mousefunction xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MouseFunction">
          <do task="leftclick"/>
        </mousefunction>
      </button>
      <button name="Right Click" number="2">
        <mousefunction xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MouseFunction">
          <do task="rightclick"/>
        </mousefunction>
      </button>
      <button name="Middle Click" number="3">
        <mousefunction xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MouseFunction">
          <do task="middleclick"/>
        </mousefunction>
      </button>
      <button name="Next Track" number="4">
        <media xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/Media">
          <do task="nexttrack"/>
        </media>
      </button>
      <button name="Play/Pause" number="5">
        <media xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/Media">
          <do task="playpause"/>
        </media>
      </button>
      <button name="Volume Down" number="6">
        <media xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/Media">
          <do task="volumedown"/>
        </media>
      </button>
      <button name="Volume Up" number="7">
        <media xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/Media">
          <do task="volumeup"/>
        </media>
      </button>
      <button name="Battery Level (G700s)" number="8">
        <mousefunction xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MouseFunction">
          <do task="batterylevel"/>
        </mousefunction>
      </button>
      <button name="DPI Down" number="9">
        <mousefunction xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MouseFunction">
          <do task="dpidown"/>
        </mousefunction>
      </button>
      <button name="DPI Up" number="10">
        <mousefunction xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MouseFunction">
          <do task="dpiup"/>
        </mousefunction>
      </button>
      <button name="Switch Profile" number="11">
        <mousefunction xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MouseFunction">
          <do task="switchprofile"/>
        </mousefunction>
      </button>
      <button name="Scroll Left" number="12">
        <mousefunction xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MouseFunction">
          <do task="scrollleft"/>
        </mousefunction>
      </button>
      <button name="Scroll Right" number="13">
        <mousefunction xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MouseFunction">
          <do task="scrollright"/>
        </mousefunction>
      </button>
    </buttons>
  </profile>
</obprofiles>
