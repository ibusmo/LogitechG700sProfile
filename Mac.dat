<?xml version="1.0" encoding="utf-8"?>
<obprofiles xmlns="http://www.logitech.com/Cassandra/2013.1/OnboardProfile">
  <profile name="Mac">
    <device anglesnapping="0" model="Logitech.Gaming.Mouse.G700s" buttoncount="13" powermode="1" reportrate="125">
      <dpitable currentindex="1" defaultindex="1">
        <dpi x="800" y="800"/>
        <dpi x="1200" y="1200"/>
        <dpi x="1600" y="1600"/>
        <dpi x="3200" y="3200"/>
        <dpi x="6400" y="6400"/>
      </dpitable>
    </device>
    <buttons>
      <button name="Left Click" number="1">
        <mousefunction xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MouseFunction">
          <do task="leftclick"/>
        </mousefunction>
      </button>
      <button name="Right Click" number="2">
        <mousefunction xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MouseFunction">
          <do task="rightclick"/>
        </mousefunction>
      </button>
      <button name="Spotlight" number="3">
        <multikey xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MultiKey">
          <key direction="down" value="F18"/>
          <key direction="up" value="F18"/>
        </multikey>
      </button>
      <button name="SwitchLaguage" number="4">
        <multikey xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MultiKey">
          <key direction="down" value="F19"/>
          <key direction="up" value="F19"/>
        </multikey>
      </button>
      <button name="ShowAllProgram" number="5">
        <multikey xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MultiKey">
          <key direction="down" value="LCTRL"/>
          <key direction="down" value="UP"/>
          <key direction="up" value="UP"/>
          <key direction="up" value="LCTRL"/>
        </multikey>
      </button>
      <button name="Volume Down" number="6">
        <media xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/Media">
          <do task="volumedown"/>
        </media>
      </button>
      <button name="Volume Up" number="7">
        <media xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/Media">
          <do task="volumeup"/>
        </media>
      </button>
      <button name="Battery Level (G700s)" number="8">
        <mousefunction xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MouseFunction">
          <do task="batterylevel"/>
        </mousefunction>
      </button>
      <button name="DPI Down" number="9">
        <mousefunction xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MouseFunction">
          <do task="dpidown"/>
        </mousefunction>
      </button>
      <button name="DPI Up" number="10">
        <mousefunction xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MouseFunction">
          <do task="dpiup"/>
        </mousefunction>
      </button>
      <button name="Switch Profile" number="11">
        <mousefunction xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MouseFunction">
          <do task="switchprofile"/>
        </mousefunction>
      </button>
      <button name="Back" number="12">
        <multikey xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MultiKey">
          <key direction="down" value="LGUI"/>
          <key direction="down" value="LBRACKET"/>
          <key direction="up" value="LBRACKET"/>
          <key direction="up" value="LGUI"/>
        </multikey>
      </button>
      <button name="Next" number="13">
        <multikey xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MultiKey">
          <key direction="down" value="LGUI"/>
          <key direction="down" value="RBRACKET"/>
          <key direction="up" value="RBRACKET"/>
          <key direction="up" value="LGUI"/>
        </multikey>
      </button>
    </buttons>
  </profile>
</obprofiles>
